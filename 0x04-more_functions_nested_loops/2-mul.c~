#include "main.h"
#include <stdio.h>



/**
 * mul - function multiplies two integers
 * @a: first argument
 * @b: second argument
 *
 * Return: 0 if function execute succesfully
 */

int mul(int a, int b)
{
return (a * b);
}

int main(void)
{
    printf("%d\n", mul(98, 1024));
    printf("%d\n", mul(-402, 4096));
    return (0);
}
