#include "main.h"
#include <stdio.h>
#include <string.h>



void puts2(char *str)
{
  int i;
  for (i = 0; i < strlen(str); i++)
    {
      if (i % 2 == 0 || i == 0)
	putchar(str[i]);
    }
  putchar('\n');
}


int main(void)
{
    char *str;

    str = "0123456789";
    puts2(str);
    return (0);
}
